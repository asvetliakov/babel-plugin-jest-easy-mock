{
  "name": "babel-plugin-jest-easy-mock",
  "version": "1.0.0",
  "description": "Easy jest mocking by automatically inlining module paths",
  "main": "dist/index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "clean": "del-cli ./dist",
    "prepare": "run-s clean build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/asvetliakov/babel-plugin-jest-easy-mock.git"
  },
  "author": "Alexey Svetliakov <asvetliakov@teledini.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/babel-core": "^6.25.3",
    "@types/jest": "^21.1.5",
    "babel-core": "^6.26.0",
    "del-cli": "^1.1.0",
    "jest": "^21.2.1",
    "npm-run-all": "^4.1.1",
    "ts-jest": "^21.1.4",
    "typescript": "^2.6.1",
    "typescript-snapshots-plugin": "^1.0.1"
  },
  "peerDependencies": {
    "babel-core": ">=6.0"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "skipBabel": true
      }
    },
    "testEnvironment": "node",
    "resetMocks": true,
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "transform": {
      "\\.(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleFileExtensions": [
      "json",
      "js",
      "ts"
    ],
    "mapCoverage": true
  }
}
